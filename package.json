{
  "name": "querybuilder",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm",
  "scripts": {
    "dev:client": "cd client && vite",
    "dev:server": "node server/server.js",
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "build": "cd client && npm install && vite build && mv dist ../dist",
    "vercel-build": "cd client && npm install && vite build && mv dist ../dist",
    "start": "node server/server.js",
    "lint": "eslint .",
    "preview": "cd client && vite preview",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": "",
    "pnpm": ""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.31.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "mysql2": "^3.7.0",
    "pg": "^8.11.3",
    "winston": "^3.10.0"
  }
}
