{
  "name": "querybuilder",
  "version": "1.0.0",
  "private": true,
  "description": "A flexible query builder for multiple databases",
  "type": "module",
  "packageManager": "pnpm",
  "scripts": {
    "dev:client": "cd client && vite",
    "dev:server": "node server/server.js",
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "build": "cd client && npm install && vite build && mv dist ../dist",
    "vercel-build": "cd client && npm install && vite build && mv dist ../dist",
    "start": "node server/server.js",
    "lint": "eslint .",
    "preview": "cd client && vite preview",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "",
    "@vitest/ui": "",
    "class-variance-authority": "",
    "clsx": "",
    "concurrently": "^8.2.2",
    "eslint": "",
    "tailwind-merge": "",
    "tailwindcss-animate": "",
    "typescript": "^5.3.3",
    "vitest": ""
  },
  "engines": {
    "node": "",
    "pnpm": ""
  },
  "dependencies": {
    "@radix-ui/react-dialog": "",
    "@radix-ui/react-dropdown-menu": "",
    "@radix-ui/react-slot": "",
    "@radix-ui/react-toast": "",
    "@supabase/supabase-js": "",
    "@tanstack/react-table": "",
    "cors": "",
    "dotenv": "",
    "express": "",
    "file-saver": "",
    "lucide-react": "",
    "mongodb": "",
    "mysql2": "",
    "node-fetch": "",
    "pg": "",
    "recharts": "",
    "winston": ""
  }
}
